#include <stdio.h>
using namespace std;
int main(){
    int s;
    printf("please input a score:");
    scanf("%d",&s);s=s/10;
    #if 0
    if(s>80){
        printf("your score level is A\n");
    }else if(s<=80 && s>70)
        printf("your score level is B\n");
    }else if(s<=70 && s>60)
        printf("your score level is C\n");
    }else if(s<=60 && s>40)
        printf("your score level is D\n");
    }else{
        printf("your score level is E\n");
    }
    #else
    switch(s){
        case 10:printf("your score level is A\n");
        break;
        case 9:printf("your score level is A\n");
        break;
        case 8:printf("your score level is A\n");
        break;
        case 7:printf("your score level is B\n");
        break;
        case 6:printf("your score level is C\n");
        break;
        case 5:printf("your score level is D\n");
        break;
        case 4:printf("your score level is D\n");
        break;
        default:printf("your score level is E\n");
        break;
    }
    #endif
    return 0;
    }
